<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.lagou.products.mapper.ProductsMapper">

    <insert id="insert" parameterType="com.lagou.common.pojo.Products" useGeneratedKeys="true" keyProperty="id">
        insert into products values(null,#{name},#{price},#{flag},#{goodsDesc},#{images},#{goodsStock},#{goodsType})
    </insert>
</mapper>